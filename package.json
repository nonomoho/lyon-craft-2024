{
  "name": "lyon-craft2024",
  "version": "0.0.0",
  "private": true,
  "description": "lyon-craft-2024",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.11.1"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "jest": "jest src/test/javascript/spec --logHeapUsage --maxWorkers=2 --no-cache",
    "test": "npm run jest --",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "eslint:ci": "eslint './**/*.{ts,js}'",
    "eslint": "eslint './**/*.{ts,js}' --fix",
    "prepare": "husky install",
    "prettier:check": "prettier --check '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "prettier:format": "prettier --write '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'"
  },
  "devDependencies": {
    "jest": "29.7.0",
    "@types/jest": "29.5.12",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "eslint": "8.56.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "@prettier/plugin-xml": "3.3.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-gherkin": "2.2.1",
    "prettier-plugin-java": "2.5.0",
    "prettier-plugin-packagejson": "2.4.11"
  }
}
