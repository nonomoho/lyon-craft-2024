{
  "name": "lyon-craft2024",
  "version": "0.0.0",
  "private": true,
  "description": "lyon-craft-2024",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.11.1"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json --noEmit && vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint --ext .js,.ts,.vue src",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:watch": "vitest --",
    "jest": "jest src/test/javascript/spec --logHeapUsage --maxWorkers=2 --no-cache",
    "test:watch:all": "jest --watchAll",
    "eslint:ci": "eslint './**/*.{ts,js}'",
    "eslint": "eslint './**/*.{ts,js}' --fix",
    "prepare": "husky install",
    "prettier:check": "prettier --check '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "prettier:format": "prettier --write '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.7.2",
    "@typescript-eslint/parser": "7.0.2",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/test-utils": "2.4.4",
    "@vitest/coverage-istanbul": "1.3.1",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.21.1",
    "jsdom": "24.0.0",
    "typescript": "5.3.3",
    "vite": "5.1.3",
    "vitest": "1.3.1",
    "vitest-sonar-reporter": "1.1.0",
    "vue-tsc": "1.8.27",
    "@types/sinon": "17.0.3",
    "sinon": "17.0.1",
    "jest": "29.7.0",
    "@types/jest": "29.5.12",
    "ts-jest": "29.1.2",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "@prettier/plugin-xml": "3.3.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-gherkin": "2.2.1",
    "prettier-plugin-java": "2.5.0",
    "prettier-plugin-packagejson": "2.4.11"
  },
  "dependencies": {
    "vue": "3.4.19",
    "axios": "1.6.7",
    "vue-router": "4.2.5"
  }
}
